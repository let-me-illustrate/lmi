                        <growablerows>0,1,2,3</growablerows>
                        <growablecols>0</growablecols>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxALL</flag>
