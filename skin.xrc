<object class="wxPanel" name="plan_panel">
    <object class="wxFlexGridSizer">
        <cols>2</cols>
        <growablecols>0,1</growablecols>
        <growablerows>0</growablerows>
        <object class="sizeritem">
            <flag>wxGROW|wxALL</flag>
            <border>4</border>
            <object class="wxFlexGridSizer">
                <cols>1</cols>
                <growablecols>0</growablecols>
                <growablerows>1</growablerows>
                <object class="sizeritem">
                    <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxLEFT</flag>
                    <border>4</border>
                    <object class="wxStaticText">
                        <label>Policy</label>
                    </object>
                </object>
                <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <border>4</border>
                    <object class="wxListBox" name="ProductName">
                        <help>Product</help>
                        <size>90,-1d</size>
                        <style>wxLB_SINGLE|wxLB_SORT</style>
                    </object>
                </object>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxGROW|wxALL</flag>
            <object class="wxFlexGridSizer">
                <rows>2</rows>
                <growablecols>0</growablecols>
                <growablerows>0,1</growablerows>
                <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <object class="wxFlexGridSizer">
                        <cols>2</cols>
                        <growablecols>0,1</growablecols>
                        <growablerows>0</growablerows>
                        <object class="sizeritem">
                            <flag>wxALIGN_LEFT|wxALIGN_TOP|wxALL</flag>
                            <border>2</border>
                            <object class="wxRadioBox" name="AvoidMecMethod">
                                <help>MEC avoidance</help>
                                <style>wxRA_SPECIFY_COLS</style>
                                <label>MEC avoidance</label>
                                <dimension>1</dimension>
                                <selection>0</selection>
<!-- SOMEDAY !! This should become a checkbox. -->
                                <content>
                                    <item helptext="Do not try to avoid a MEC">Allow MEC</item>
                                    <item helptext="Avoid a MEC by decreasing premium">Reduce premium</item>
                                </content>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxALIGN_RIGHT|wxALIGN_TOP|wxALL</flag>
                            <border>2</border>
                            <object class="wxRadioBox" name="DefinitionOfLifeInsurance">
                                <help>Definition of life insurance</help>
                                <style>wxRA_SPECIFY_COLS</style>
                                <label>Definition of life insurance</label>
                                <dimension>1</dimension>
                                <selection>0</selection>
                                <content>
                                    <item helptext="Definition of life insurance is cash value accumulation test">CVAT</item>
                                    <item helptext="Definition of life insurance is guideline premium test">GPT</item>
                                    <item helptext="Do not comply with US definition of life insurance">Not 7702 compliant</item>
                                </content>
                            </object>
                        </object>
                    </object>
                </object>
                <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <object class="wxFlexGridSizer">
                        <cols>2</cols>
                        <growablecols>0,1</growablecols>
                        <growablerows>0</growablerows>
                        <object class="sizeritem">
                            <flag>wxALIGN_LEFT|wxALIGN_BOTTOM|wxALL</flag>
                            <border>2</border>
                            <object class="wxRadioBox" name="DefinitionOfMaterialChange">
                                <help>Definition of material change</help>
                                <style>wxRA_SPECIFY_COLS</style>
                                <label>Material change definition</label>
                                <dimension>1</dimension>
                                <selection>0</selection>
                                <content>
                                    <item helptext="Recognize material change whenever unnecessary premium is paid">Unnecessary premium</item>
                                    <item helptext="Recognize material change whenever benefit increases">Benefit increase</item>
                                    <item helptext="Recognize material change upon the later of unnecessary premium or benefit increase">Later of increase or unnecessary premium</item>
                                    <item helptext="Recognize material change upon the earlier of unnecessary premium or benefit increase">Earlier of increase or unnecessary premium</item>
                                    <item helptext="Recognize material change at each guideline premium adjustment event">GPT adjustment event</item>
                                </content>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <border>2</border>
                            <object class="wxFlexGridSizer">
                                <cols>1</cols>
                                <growablecols>0</growablecols>
                                <growablerows>0,1,2,3,4</growablerows>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxALL</flag>
                                    <object class="wxFlexGridSizer">
                                        <rows>1</rows>
                                        <growablecols>1</growablecols>
                                        <object class="sizeritem">
                                            <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxLEFT</flag>
                                            <border>4</border>
                                            <object class="wxStaticText">
                                                <label>NAAR limit </label>
                                            </object>
                                        </object>
                                        <object class="sizeritem">
                                            <flag>wxGROW</flag>
                                            <object class="wxTextCtrl" name="MaximumNaar">
                                                <help>Maximum net amount at risk, typically imposed by reinsurer; sufficient account value is paid out each month to honor this limit</help>
                                                <size>10,-1</size>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxALIGN_LEFT|wxALL</flag>
                                    <border>4</border>
                                    <object class="wxCheckBox" name="EffectiveDateToday">
                                        <help>Synchronize effective date with current date</help>
                                        <label>Effective today</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxALL</flag>
                                    <border>4</border>
                                    <object class="wxFlexGridSizer">
                                        <rows>1</rows>
                                        <growablecols>1</growablecols>
                                        <object class="sizeritem">
                                            <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxLEFT</flag>
                                            <object class="wxStaticText">
                                                <label>Effective date </label>
                                            </object>
                                        </object>
                                        <object class="sizeritem">
                                            <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                            <object class="wxDatePickerCtrl" name="EffectiveDate">
                                                <help>Effective date</help>
                                                <size>90,-1</size>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxALL</flag>
                                    <object class="wxFlexGridSizer">
                                        <rows>1</rows>
                                        <growablecols>0</growablecols>
                                        <object class="sizeritem">
                                            <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxLEFT</flag>
                                            <border>4</border>
                                            <object class="wxStaticText">
                                                <label>State of jurisdiction</label>
                                            </object>
                                        </object>
                                        <object class="sizeritem">
                                            <flag>wxGROW</flag>
                                            <object class="wxComboBox" name="StateOfJurisdiction">
                                                <help>State that approves policy form and rate filing, and governs everything else except premium tax</help>
                                                <style>wxCB_DROPDOWN|wxCB_READONLY</style>
                                                <content><item>MM</item><item>WW</item></content>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxALL</flag>
                                    <object class="wxFlexGridSizer">
                                        <rows>1</rows>
                                        <growablecols>0</growablecols>
                                        <object class="sizeritem">
                                            <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxLEFT</flag>
                                            <border>4</border>
                                            <object class="wxStaticText">
                                                <label>Premium tax state</label>
                                            </object>
                                        </object>
                                        <object class="sizeritem">
                                            <flag>wxGROW</flag>
                                            <object class="wxComboBox" name="PremiumTaxState">
                                                <help>State to which premium tax is paid--affects premium load if the tax is passed through</help>
                                                <style>wxCB_DROPDOWN|wxCB_READONLY</style>
                                                <content><item>MM</item><item>WW</item></content>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL|wxRIGHT</flag>
            <border>4</border>
            <object class="wxStaticText">
                <label>Comments</label>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxGROW</flag>
            <object class="wxTextCtrl" name="Comments">
                <help>Freeform comments</help>
                <size>180,-1</size>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL|wxRIGHT</flag>
            <border>4</border>
            <object class="wxStaticText">
                <label>Extra policy fee</label>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxGROW</flag>
            <object class="InputSequenceEntry" name="ExtraMonthlyCustodialFee">
                <title>Additional monthly policy fee</title>
                <help>Additional monthly policy fee (e.g., for fund custodian charges)</help>
                <size>180,-1</size>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL|wxRIGHT</flag>
            <border>4</border>
            <object class="wxStaticText">
                <label>Extra separate-account asset charge</label>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxGROW</flag>
            <object class="InputSequenceEntry" name="ExtraCompensationOnAssets">
                <title>Additional separate-account asset charge</title>
                <help>Additional separate-account asset charge (e.g., for negotiated asset-based compensation)</help>
                <size>180,-1</size>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL|wxRIGHT</flag>
            <border>4</border>
            <object class="wxStaticText">
                <label>Extra premium load</label>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxGROW</flag>
            <object class="InputSequenceEntry" name="ExtraCompensationOnPremium">
                <title>Additional premium load</title>
                <help>Additional premium load (e.g., for negotiated premium-based compensation)</help>
                <size>180,-1</size>
            </object>
        </object>
    </object>
</object>

<object class="wxPanel" name="group_panel">
    <object class="wxFlexGridSizer">
        <rows>2</rows>
        <growablecols>0</growablecols>
        <growablerows>0</growablerows>
        <object class="sizeritem">
            <flag>wxGROW|wxALL</flag>
            <object class="wxFlexGridSizer">
                <cols>3</cols>
                <growablecols>1</growablecols>
                <growablerows>0</growablerows>
                <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <object class="wxFlexGridSizer">
                        <cols>1</cols>
                        <growablecols>0</growablecols>
                        <growablerows>0,1</growablerows>
                        <object class="sizeritem">
                            <flag>wxALIGN_LEFT|wxALIGN_TOP|wxALL</flag>
                            <border>2</border>
                            <object class="wxRadioBox" name="GroupUnderwritingType">
                                <help>Group underwriting</help>
                                <style>wxRA_SPECIFY_COLS</style>
                                <label>Group underwriting</label>
                                <dimension>1</dimension>
                                <selection>0</selection>
                                <content>
                                    <item>Medical</item>
                                    <item>Paramedical</item>
                                    <item>Nonmedical</item>
                                    <item>Simplified issue</item>
                                    <item>Guaranteed issue</item>
                                </content>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxALIGN_LEFT|wxALIGN_TOP|wxALL</flag>
                            <border>2</border>
                            <object class="wxStaticBoxSizer">
                                <orient>wxVERTICAL</orient>
                                <label>Blend mortality</label>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxALL</flag>
                                    <object class="wxFlexGridSizer">
                                        <cols>2</cols>
                                        <growablecols>1</growablecols>
                                        <growablerows>0,1</growablerows>
                                        <object class="sizeritem">
                                            <flag>wxGROW|wxALL</flag>
                                            <border>4</border>
                                            <object class="wxCheckBox" name="BlendGender">
                                                <help>Blend mortality by gender</help>
                                                <label>Male</label>
                                            </object>
                                        </object>
                                        <object class="sizeritem">
                                            <flag>wxGROW|wxALL</flag>
                                            <border>4</border>
                                            <object class="wxTextCtrl" name="MaleProportion">
                                                <help>Male percentage for mortality blending</help>
                                                <size>40,-1</size>
                                                <value>1</value>
                                            </object>
                                        </object>
                                        <object class="sizeritem">
                                            <flag>wxGROW|wxALL</flag>
                                            <border>4</border>
                                            <object class="wxCheckBox" name="BlendSmoking">
                                                <help>Blend mortality by smoking</help>
                                                <label>Nontobacco</label>
                                            </object>
                                        </object>
                                        <object class="sizeritem">
                                            <flag>wxGROW|wxALL</flag>
                                            <border>4</border>
                                            <object class="wxTextCtrl" name="NonsmokerProportion">
                                                <help>Nonsmoker percentage for mortality blending</help>
                                                <size>40,-1</size>
                                                <value>1</value>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
                <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <border>4</border>
                    <object class="wxFlexGridSizer">
                        <cols>1</cols>
                        <growablecols>0</growablecols>
                        <growablerows>0,1,2,3,4,5,6,7</growablerows>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <object class="wxFlexGridSizer">
                                <rows>1</rows>
                                <growablecols>1</growablecols>
                                <object class="sizeritem">
                                    <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                    <object class="wxStaticText">
                                        <label>Cell number of identical lives </label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW</flag>
                                    <object class="wxTextCtrl" name="NumberOfIdenticalLives">
                                        <help>Number of identical lives represented by this cell</help>
                                        <size>10,-1</size>
                                    </object>
                                </object>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <object class="wxFlexGridSizer">
                                <rows>1</rows>
                                <growablecols>1</growablecols>
                                <object class="sizeritem">
                                    <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                    <object class="wxStaticText">
                                        <label>Last COI reentry date </label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                    <object class="wxDatePickerCtrl" name="LastCoiReentryDate">
                                        <help>Last COI reentry date</help>
                                        <size>90,-1</size>
                                    </object>
                                </object>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <object class="wxFlexGridSizer">
                                <rows>1</rows>
                                <growablecols>1</growablecols>
                                <object class="sizeritem">
                                    <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                    <object class="wxStaticText">
                                        <label>List bill date </label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                    <object class="wxDatePickerCtrl" name="ListBillDate">
                                        <help>List bill date</help>
                                        <size>90,-1</size>
                                    </object>
                                </object>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <object class="wxCheckBox" name="IncludeInComposite">
                                <help>Include this individual in any group composite</help>
                                <label>Include in composite</label>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <object class="wxCheckBox" name="RetireesCanEnroll">
                                <help>Let individuals enroll after their retirement</help>
                                <label>Retirees eligible to enroll</label>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <object class="wxCheckBox" name="AmortizePremiumLoad">
                                <help>Amortize all premium load components; charge an extra interest spread instead</help>
                                <label>Amortize premium load</label>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <object class="wxCheckBox" name="UsePartialMortality">
                                <help>Use partial mortality (for group composite only)</help>
                                <label>Partial mortality</label>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <object class="wxFlexGridSizer">
                                <rows>1</rows>
                                <growablecols>1</growablecols>
                                <object class="sizeritem">
                                    <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                    <object class="wxStaticText">
                                        <label>Partial mortality table </label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW</flag>
                                    <object class="wxComboBox" name="PartialMortalityTable">
                                        <help>Partial mortality table</help>
                                        <style>wxCB_DROPDOWN|wxCB_READONLY</style>
                                        <selection>0</selection>
                                        <content>
                                            <item>83GAM</item>
                                        </content>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
                <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <object class="wxFlexGridSizer">
                        <cols>1</cols>
                        <growablecols>0</growablecols>
                        <growablerows>0,1</growablerows>
                        <object class="sizeritem">
                            <object class="wxRadioBox" name="RunOrder">
                                <help>Group processing order</help>
                                <style>wxRA_SPECIFY_COLS</style>
                                <label>Processing order</label>
                                <content>
                                    <item helptext="Run a group of cells in series">Life by life</item>
                                    <item helptext="Run a group of cells in parallel">Month by month</item>
                                </content>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <object class="wxRadioBox" name="SurviveToType">
                                <help>Survival limit for partial mortality</help>
                                <style>wxRA_SPECIFY_COLS</style>
                                <label>Survival limit</label>
                                <content>
                                    <item helptext="Partial mortality: no particular limit on survival">No survival limit</item>
                                    <item helptext="Partial mortality: survival limited to input age">Survive to age limit</item>
                                    <item helptext="Partial mortality: survival limited to input duration">Survive to duration limit</item>
                                    <item helptext="Partial mortality: survival limited to life expectancy">Survive to life expectancy</item>
                                </content>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <object class="wxFlexGridSizer">
                                <cols>2</cols>
                                <growablecols>1</growablecols>
                                <object class="sizeritem">
                                    <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                    <object class="wxStaticText">
                                        <label>Age </label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW</flag>
                                    <object class="wxSpinCtrl" name="SurviveToAge">
                                        <help>Partial mortality survival age limit</help>
                                        <style>wxSP_ARROW_KEYS</style>
                                        <max>100</max>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                    <object class="wxStaticText">
                                        <label>Year </label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW</flag>
                                    <object class="wxSpinCtrl" name="SurviveToYear">
                                        <help>Partial mortality survival year limit</help>
                                        <style>wxSP_ARROW_KEYS</style>
                                        <max>100</max>
                                        <value>99</value>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxGROW|wxALL</flag>
            <border>4</border>
            <object class="wxFlexGridSizer">
                <cols>2</cols>
                <growablecols>1</growablecols>
                <object class="sizeritem">
                    <flag>wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL|wxRIGHT</flag>
                    <border>4</border>
                    <object class="wxStaticText">
                        <label>Current COI multiplier</label>
                    </object>
                </object>
                <object class="sizeritem">
                    <flag>wxGROW</flag>
                    <object class="InputSequenceEntry" name="CurrentCoiMultiplier">
                        <title>Multiplier for current COI rates</title>
                        <help>Multiplier for current COI rates</help>
                        <size>180,-1</size>
                    </object>
                </object>
                <object class="sizeritem">
                    <flag>wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL|wxRIGHT</flag>
                    <border>4</border>
                    <object class="wxStaticText">
                        <label>Partial mortality multiplier</label>
                    </object>
                </object>
                <object class="sizeritem">
                    <flag>wxGROW</flag>
                    <object class="InputSequenceEntry" name="PartialMortalityMultiplier">
                        <title>Multiplier for partial mortality table</title>
                        <help>Multiplier for partial mortality table</help>
                        <size>180,-1</size>
                    </object>
                </object>
            </object>
        </object>
    </object>
</object>

<object class="wxPanel" name="names_panel">
    <object class="wxFlexGridSizer">
        <cols>1</cols>
        <growablecols>0</growablecols>
        <growablerows>0,1,2</growablerows>
        <!-- insured -->
        <object class="sizeritem">
            <flag>wxGROW|wxALL</flag>
            <object class="wxStaticBoxSizer">
                <orient>wxVERTICAL</orient>
                <label>Insured</label>
                <object class="sizeritem">
                    <flag>wxGROW|wxALL</flag>
                    <border>2</border>
                    <object class="wxFlexGridSizer">
                        <rows>1</rows>
                        <growablecols>1</growablecols>
                        <object class="sizeritem">
                            <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                            <border>4</border>
                            <object class="wxStaticText">
                                <label>Name</label>
                            </object>
                        </object>
