<object class="wxPanel" name="plan_panel">
    <title>Plan</title>
    <centered>1</centered>
    <object class="wxFlexGridSizer">
        <cols>1</cols>
        <rows>2</rows>
        <growablecols>0</growablecols>
        <growablerows>0,1</growablerows>
        <object class="sizeritem">
            <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>4</border>
            <object class="wxFlexGridSizer">
                <rows>1</rows>
                <growablecols>1,2</growablecols>
                <growablerows>0</growablerows>
                <object class="sizeritem">
                    <flag>wxALIGN_LEFT|wxGROW|wxLEFT|wxTOP</flag>
                    <border>4</border>
                    <object class="wxFlexGridSizer">
                        <cols>1</cols>
                        <rows>0</rows>
                        <growablecols>0</growablecols>
                        <growablerows>1</growablerows>
                        <object class="sizeritem">
                            <flag>wxALIGN_LEFT|wxALIGN_TOP|wxLEFT|wxRIGHT|wxTOP</flag>
                            <border>4</border>
                            <object class="wxStaticText">
                                <label>Product</label>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <border>4</border>
                            <object class="wxListBox" name="ProductName">
                                <help>Product</help>
                                <size>-1,0</size>
                                <style>wxLB_SINGLE</style>
                            </object>
                        </object>
                    </object>
                </object>
                <object class="sizeritem">
                    <flag>wxALIGN_LEFT|wxGROW|wxTOP</flag>
                    <border>15</border>
                    <object class="wxFlexGridSizer">
                        <cols>1</cols>
                        <rows>0</rows>
                        <growablecols>0</growablecols>
                        <growablerows>0</growablerows>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALL</flag>
                            <border>2</border>
                            <object class="wxRadioBox" name="DefinitionOfLifeInsurance">
                                <help>Definition of life insurance</help>
                                <style>wxRA_SPECIFY_COLS</style>
                                <label>Definition of life insurance</label>
                                <content>
                                    <item helptext="Definition of life insurance is cash value accumulation test">CVAT</item>
                                    <item helptext="Definition of life insurance is guideline premium test">GPT</item>
                                    <item helptext="Do not comply with US definition of life insurance">Not 7702 compliant</item>
                                </content>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                            <border>2</border>
                            <object class="wxRadioBox" name="DefinitionOfMaterialChange">
                                <help>Definition of material change</help>
                                <style>wxRA_SPECIFY_COLS</style>
                                <label>Material change definition</label>
                                <content>
                                    <item helptext="Recognize material change whenever unnecessary premium is paid">Unnecessary premium</item>
                                    <item helptext="Recognize material change whenever benefit increases">Benefit increase</item>
                                    <item helptext="Recognize material change upon the later of unnecessary premium or benefit increase">Later of increase or unnecessary premium</item>
                                    <item helptext="Recognize material change upon the earlier of unnecessary premium or benefit increase">Earlier of increase or unnecessary premium</item>
                                    <item helptext="Recognize material change at each guideline premium adjustment event">GPT adjustment event</item>
                                </content>
                            </object>
                        </object>
                    </object>
                </object>
                <object class="sizeritem">
                    <flag>wxGROW|wxLEFT|wxTOP</flag>
                    <border>13</border>
                    <object class="wxFlexGridSizer">
                        <cols>1</cols>
                        <rows>0</rows>
                        <growablecols>0</growablecols>
                        <growablerows>0,1,2</growablerows>
                        <object class="sizeritem">
                            <flag>wxRIGHT|wxTOP|wxBOTTOM</flag>
                            <border>4</border>
                            <object class="wxStaticBoxSizer">
                                <orient>wxVERTICAL</orient>
                                <label>Effective date</label>
                                <object class="sizeritem">
                                    <flag>wxALIGN_LEFT|wxALL</flag>
                                    <border>4</border>
                                    <object class="wxFlexGridSizer">
                                        <cols>0</cols>
                                        <rows>1</rows>
                                        <growablecols>1</growablecols>
                                        <object class="sizeritem">
                                            <flag>wxALIGN_LEFT|wxALIGN_TOP|wxALL</flag>
                                            <border>4</border>
                                            <object class="wxDatePickerCtrl" name="EffectiveDate">
                                                <help>Effective date</help>
                                                <size>90,-1</size>
                                            </object>
                                        </object>
                                        <object class="sizeritem">
                                            <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                            <border>4</border>
                                            <object class="wxCheckBox" name="EffectiveDateToday">
                                                <help>Set effective date to current date</help>
                                                <label>Today</label>
                                                <checked>0</checked>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
<!--
This doesn't actually do anything today, but it'll probably be
useful in the future.
                        <object class="sizeritem">
                            <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                            <border>4</border>
                            <object class="wxFlexGridSizer">
                                <cols>0</cols>
                                <rows>1</rows>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxLEFT|wxTOP|wxBOTTOM</flag>
                                    <border>4</border>
                                    <object class="wxStaticText">
                                        <label>State of jurisdiction</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxLEFT|wxRIGHT</flag>
                                    <border>4</border>
                                    <object class="wxComboBox" name="StateOfJurisdiction">
                                        <size>50,20</size>
                                        <style>wxCB_READONLY</style>
                                    </object>
                                </object>
                            </object>
                        </object>
-->
<!-- Users want this field masked to avoid input confusion until
the real 'StateOfJurisdiction' is implemented.
-->
                        <object class="sizeritem">
                            <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                            <border>4</border>
                            <object class="wxFlexGridSizer">
                                <cols>0</cols>
                                <rows>1</rows>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxLEFT|wxTOP|wxBOTTOM</flag>
                                    <border>4</border>
                                    <object class="wxStaticText">
                                        <label>State of jurisdiction</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxLEFT|wxRIGHT</flag>
                                    <border>4</border>
                                    <object class="wxComboBox" name="State">
                                        <help>Insured state</help>
                                        <size>50,20</size>
                                        <style>wxCB_READONLY</style>
                                    </object>
                                </object>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxLEFT|wxRIGHT|wxTOP</flag>
                            <border>4</border>
                            <object class="wxRadioBox" name="RunOrder">
                                <help>Group processing order</help>
                                <style>wxRA_SPECIFY_ROWS</style>
                                <label>Processing order</label>
                                <content>
                                    <item helptext="Run a group of cells in series">Life by life</item>
                                    <item helptext="Run a group of cells in parallel">Month by month</item>
                                </content>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxALIGN_LEFT|wxGROW|wxLEFT|wxRIGHT|wxBOTTOM</flag>
