                                        <label>Class</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxALIGN_RIGHT|wxGROW|wxALL</flag>
                                    <border>4</border>
                                    <object class="wxComboBox" name="UnderwritingClass">
                                        <help>Underwriting class</help>
                                        <size>100,20</size>
                                        <style>wxCB_READONLY</style>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                    <border>4</border>
                                    <object class="wxStaticText">
                                        <label>Tobacco</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxALL</flag>
                                    <border>4</border>
                                    <object class="wxComboBox" name="Smoking">
                                        <help>Smoking</help>
                                        <size>100,20</size>
                                        <style>wxCB_READONLY</style>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                    <border>4</border>
                                    <object class="wxStaticText">
                                        <label>Rating</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxALL</flag>
                                    <border>4</border>
                                    <object class="wxComboBox" name="SubstandardTable">
                                        <help>Substandard table rating</help>
                                        <size>100,20</size>
                                        <style>wxCB_READONLY</style>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                    <border>4</border>
                                    <object class="wxStaticText">
                                        <label>Flat extra</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <flag>wxGROW|wxLEFT|wxRIGHT|wxTOP</flag>
                                    <border>4</border>
                                    <object class="wxTextCtrl" name="FlatExtra">
                                        <help>Flat extra: annual amount per thousand</help>
                                    </object>
                               </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
        </object>
    </object>
</object>

