        </object>
<!-- TODO ?? Change the 'name' too. -->
        <object class="wxMenuItem" name="shrink_columns">
            <label>_Use fixed column width\tCtrl-U</label>
            <bitmap>fixed-width.xpm</bitmap>
            <help>Display all columns with the same fixed width</help>
        </object>
    </object>
